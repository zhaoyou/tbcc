<hibernate-mapping>
    <class name="org.fdap.entity.Fdaplinktype" table="Fdaplinktype">
        <id name="ltid" type="java.lang.Long">
            <column name="ltid" />
            <generator class="native" />
        </id>
        <property name="ltname" type="java.lang.String">
            <column name="ltname" length="25" not-null="true" />
        </property>
        
    </class>
</hibernate-mapping>
