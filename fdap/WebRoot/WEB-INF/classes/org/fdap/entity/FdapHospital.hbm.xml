<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    zhaoyou 
-->
<hibernate-mapping>
    <class name="org.fdap.entity.FdapHospital" table="fdaphospital">
    	<id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="isReal"></property>
        <property name="latitude"></property>
        <property name="longitude"></property>
        <property name="projectId"></property>
        <property name="hospitalName"></property>
    </class>
   
    
</hibernate-mapping>
