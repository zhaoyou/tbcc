<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    administrator
-->
<hibernate-mapping>
    <class name="org.tbcc.entity.TbccRoleAndFunction" table="tbccRoleAndFunction" schema="dbo" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="roleId" type="java.lang.Integer">
            <column name="roleId" length="4" not-null="true" />
        </property>
        <property name="functionId" type="java.lang.Integer">
            <column name="functionId" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
