<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    zhaoyou 
-->
<hibernate-mapping>
    <class name="org.fdap.entity.FdapRefHisData" table="FdapRefHisData" mutable="false" >
    
    
        <id name="refHisId" type="java.lang.Long">
            <column name="refHisId"  />
            <generator class="native" />
        </id>
       
       <property name="aiGuid" type="java.lang.String" column="aiGuid"></property>
       
       <property name="data" type="java.lang.Double" column="data"></property>
       
       <property name="isAlarm" type="java.lang.Integer" column="isAlarm"></property>
       
       <property name="time" type="java.util.Date" column="time"></property>
       
    </class>
    
</hibernate-mapping>

