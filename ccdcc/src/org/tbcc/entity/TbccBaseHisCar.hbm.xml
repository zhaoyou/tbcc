			 <property name="histDataStorageType" type="java.lang.Integer" column="histDataStorageType"></property>
			 <property name="unloadStorageType" type="java.lang.Integer" column="unloadStorageType"></property>
			 <property name="histAlarmData" type="java.lang.Integer" column="histAlarmData"></property>
			 
		</class>
</hibernate-mapping>